    "connect": "^3.3.5",
    "express": "4.10.x",
    "handlebars": "^4.0.5",
    "request": "2.47.x"
  },
  "devDependencies": {
    "babel": "6.0.x",
    "babel-cli": "6.3.x",
    "babel-core": "6.3.x",
    "babel-plugin-transform-es2015-modules-amd": "6.3.x",
    "babel-plugin-transform-flow-strip-types": "6.3.x",
    "babel-preset-es2015": "6.3.x",
    "babel-preset-stage-0": "6.3.x",
    "less": "1.7.x",
    "nodeunit": "0.9.x",
